<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="i_app_assets_frontend" inherit_id="website.assets_frontend"
		name="I App Asstes">
		<xpath expr="//link[last()]" position="after">

			<!-- Website Title -->
			<title> I App | App Landing pages HTML Templates </title>
			<!-- Styles -->
			<!-- favicon -->
			<link rel="icon" href="/theme_iapp/static/src/img/favicon.png" type="image/x-icon" />
			<!-- bootstrap -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/bootstrap.min.css"
				type="text/css" />
			<!-- icofont -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/fontawesome.5.7.2.css"
				type="text/css" />
			<!-- flaticon -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/flaticon.css"
				type="text/css" />
			<!-- animate.css -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/animate.css"
				type="text/css" />
			<!-- Owl Carousel -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/owl.carousel.min.css"
				type="text/css" />
			<!-- magnific popup -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/magnific-popup.css"
				type="text/css" />
			<!-- stylesheet -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/style.css"
				type="text/css" />
			<!-- responsive -->
			<link rel="stylesheet" href="/theme_iapp/static/src/css/responsive.css"
				type="text/css" />

			<link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css"
				type="text/css" />
		</xpath>
		<xpath expr="//script[last()]" position="after">
			<!-- popper -->
			<script src="/theme_iapp/static/src/js/popper.min.js" type="text/javascript"></script>
			<!-- bootstrap -->
			<script src="/theme_iapp/static/src/js/bootstrap.min.js" type="text/javascript"></script>
			<script src="/theme_iapp/static/src/js/scroll.js" type="text/javascript"></script>
			<script src="/theme_iapp/static/src/js/blog_search.js" type="text/javascript"></script>
		</xpath>
	</template>
</odoo>
