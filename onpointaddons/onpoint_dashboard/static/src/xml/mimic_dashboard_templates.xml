<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="OnpointDashboardMimic">
        <div class="o_dashboard">
            X
        </div>
    </t>

    <t t-name="OnpointDashboardMimicContent">
        <div>
            <div class="mimic_diagram">
                <img src="/onpoint_dashboard/static/src/img/mimics/mimic1.png"/>
            </div>

            <div class="row dashboard-header">
                <div class="col-md-3 dashboard-header">
                    <img id="btn_logo" class="logo" src="/onpoint_dashboard/static/src/img/icon_logo.png"/>
