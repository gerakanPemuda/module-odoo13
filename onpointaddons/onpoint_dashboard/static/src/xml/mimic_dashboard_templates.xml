                <div class="col-md-8 dashboard-header text-right">
                    <img id="btn_main" class="icons" src="/onpoint_dashboard/static/src/img/icon_maps.png"/>
                    <img id="btn_logger_list" class="icons" src="/onpoint_dashboard/static/src/img/icon_logger_list.png"/>
<!--                    <img id="btn_mimic" class="icons" src="/onpoint_dashboard/static/src/img/icon_mimic.png"/>-->
                </div>
            </div>
        </div>

    </t>



    <t t-name="OnpointDashboardMimicWarning">
        <div>
            <p>Error : Could not find employee linked to user</p>
            <p>Please contact system admin for the setup</p>
        </div>
