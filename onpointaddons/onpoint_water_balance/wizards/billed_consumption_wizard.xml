<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="billed_consumption_wizard_form" model="ir.ui.view">
            <field name="name">billed.consumption.wizard.form</field>
            <field name="model">billed.consumption.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="worksheet" filename="name"/>
                    </group>
                    <footer>
                        <button string="Import" class="btn btn-primary" type="object"
                                name="action_billed_consumption_import" />
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>