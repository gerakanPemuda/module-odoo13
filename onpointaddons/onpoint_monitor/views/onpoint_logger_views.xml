        <record id="view_channel_values_form" model="ir.ui.view">
            <field name="name">onpoint.logger.channel.form</field>
            <field name="model">onpoint.logger.channel</field>
            <field name="arch" type="xml">
                <form create="false" delete="false">
                    <group>
                        <group>
                            <field name="value_type_id" readonly="1"/>
                            <field name="value_unit_id" readonly="1"/>
                            <field name="point_id" readonly="1"/>
                        </group>

                        <notebook colspan="4">
                            <page string="Detail">
                                <group>
                                    <field name="value_ids" nolabel="1" colspan="4" widget="one2many_list">
                                        <tree editable="bottom" create="false">
                                            <field name="dates" readonly="1"/>
                                            <field name="channel_value" readonly="1"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                        </notebook>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_onpoint_logger_kanban" model="ir.ui.view">
            <field name="name">onpoint.logger.kanban</field>
            <field name="model">onpoint.logger</field>
            <field name="type">kanban</field>
            <field name="arch" type="xml">
                <kanban string="Logger" class="o_kanban_mobile">
                    <field name="id"/>
                    <field name="name"/>
                    <field name="address"/>
                    <field name="logger_type_id"/>
                    <field name="logger_type_name"/>
                    <field name="zone_id"/>
                    <field name="dma_id"/>
                    <field name="last_data_date"/>
                    <field name="is_still_active"/>
                    <field name="state"/>
                    <field name="channel_ids"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div class="card-logger">
                                <div t-attf-class="card-header #{record.state.raw_value}">
                                    <div class="row">
                                        <div class="col-md-10">
                                            <h6>
                                                <t t-esc="record.logger_type_name.value"/>
                                            </h6>
                                            <h2>
                                                <t t-esc="record.name.value"/>
                                            </h2>
                                        </div>
                                        <div class="col-md-2 logger-state">
                                            <t t-if="record.state.raw_value == 'enabled'">
                                                <t t-if="record.is_still_active.raw_value == true">
                                                    <i class="fas fa-check-circle fa-2x"/>
                                                </t>
                                                <t t-else="">
                                                    <i class="fas fa-exclamation-triangle fa-2x"/>
                                                </t>
                                            </t>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-content">
                                    <div class="row" style="magin: auto">
                                        <t t-foreach="record.channel_ids.raw_value" t-as="channel">
                                            <t t-if="channel.display_on_chart == true">
                                                <t t-if="channel.value_unit_name">
                                                    <div class="col-md-6" style="margin-top: 5px">
                                                        <h6>
                                                            <t t-if="channel.name" t-esc="channel.name"/>
                                                            <t t-else="" t-esc="channel.value_type_name"/>
                                                        </h6>
                                                        <h3>
                                                            <t t-esc="channel.last_value"/>&#160;
                                                            <t t-esc="channel.value_unit_name"/>
                                                        </h3>
                                                    </div>
                                                </t>
                                            </t>
                                        </t>
                                    </div>
                                </div>
                                <div class="card-footer">
                                    <div class="row">
                                        <div class="col">
                                            <t t-esc="record.last_data_date.value"/>
                                        </div>
                                        <div class="col" style="text-align: right">
                                            <button t-attf-class="btn btn-primary #{record.state.raw_value}"
                                                    type="action"
                                                    name="%(act_view_logger_chart)d"
                                                    string="View">View
                                            </button>
                                        </div>

                                    </div>
                                </div>

                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <record id="view_onpoint_logger_map" model="ir.ui.view">
            <field name="name">onpoint.logger.map</field>
            <field name="model">onpoint.logger</field>
            <field name="arch" type="xml">
                <map class="oe_kanban_global_click o_res_partner_map"
                     library="geometry"
                     string="Map"
                     lat="latitude"
                     lng="longitude"
                     create="0"
                     colors="marker_seba:brand_owner=='seba';marker_pointorange:brand_owner=='pointorange';">
                    <field name="id"/>
                    <field name="brand_owner"/>
                    <field name="latitude"/>
                    <field name="longitude"/>
                    <field name="name"/>
                    <field name="address"/>
                    <field name="logger_type_id"/>
                    <field name="zone_id"/>
                    <field name="dma_id"/>
                    <field name="image_1920"/>
                    <field name="channel_ids"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_global_click oe_kanban_details">
                                <div class="oe_kanban_details" style="margin-left:10px">
                                    <div>
                                        <strong class="o_kanban_record_title">
                                            <span>
                                                <t t-esc="record.name.value"/>
                                            </span>
                                        </strong>
                                        <p>
                                            <field name="logger_type_id"/>
                                        </p>
                                    </div>
                                    <table width="200vw">
                                        <t t-foreach="record.channel_ids.raw_value" t-as="channel">
                                            <t t-if="channel.display_on_chart == true">
                                                <t t-if="channel.value_unit_name">
                                                    <tr>
                                                        <td>
                                                            <t t-esc="channel.name"/>
                                                        </td>
                                                        <td>:
                                                            <t t-esc="channel.last_value"/>
                                                            <t t-esc="channel.value_unit_name"/>
                                                        </td>
                                                    </tr>
                                                </t>
                                            </t>
                                        </t>
                                    </table>
                                    <div class="o_kanban_record_bottom float-right" style="margin-right:10px">
                                        <button class="btn btn-primary" type="action" name="%(act_view_logger_chart)d"
                                                string="View">View
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </templates>
                </map>
            </field>
        </record>

        <record id="view_onpoint_logger_search" model="ir.ui.view">
            <field name="name">onpoint.logger.search</field>
            <field name="model">onpoint.logger</field>
            <field name="arch" type="xml">
                <search string="Cari Logger">
                    <field name="name"/>
                    <field name="address"/>
                    <field name="logger_type_id"/>
                    <field name="dma_id"/>
                    <field name="department_id"/>

                    <filter string="Enabled Logger" name="filter_enabled_logger"
                            domain="[('state', '=', 'enabled')]"/>
                    <filter string="Disabled Logger" name="filter_disabled_logger"
                            domain="[('state', '=', 'disabled')]"/>

                    <!--                    <filter string="Logger Type: Critical Point" name="filter_critical_point"-->
                    <!--                            domain="[('logger_type_name', '=', 'Critical Point')]"/>-->
                    <!--                    <filter string="Logger Type: Reservoir" name="filter_reservoir"-->
                    <!--                            domain="[('logger_type_name', '=', 'Reservoir')]"/>-->
                    <!--                    <filter string="Logger Type: Meter Induk" name="filter_reservoir"-->
                    <!--                            domain="[('logger_type_name', '=', 'Meter Induk')]"/>-->
                    <!--                    <filter string="Logger Type: Rumah Pompa" name="filter_reservoir"-->
                    <!--                            domain="[('logger_type_name', '=', 'Rumah Pompa')]"/>-->
                    <!--                    <filter string="Logger Type: Fountain" name="filter_reservoir"-->
                    <!--                            domain="[('logger_type_name', '=', 'Fountain')]"/>-->
                    <!--                    <filter string="Logger Type: Pos Clorinasi" name="filter_reservoir"-->
                    <!--                            domain="[('logger_type_name', '=', 'Pos Clorinasi')]"/>-->
                    <!--                    <filter string="Logger Type: RCA" name="filter_reservoir"-->
                    <!--                            domain="[('logger_type_name', '=', 'RCA')]"/>-->

                    <searchpanel>
                        <field name="logger_type_id" string="Logger Type" icon="fa-object-group"/>
                        <field name="dma_id" string="DMA" icon="fa-layer-group"/>
                    </searchpanel>

                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_onpoint_logger">
            <field name="name">Logger</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">onpoint.logger</field>
            <field name="view_mode">map,kanban,tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{'search_default_filter_enabled_logger' : 1}</field>
            <field name="search_view_id" ref="view_onpoint_logger_search"/>
        </record>

        <record id="action_onpont_logger_map" model="ir.actions.act_window.view">
            <field eval="3" name="sequence"/>
            <field name="view_mode">map</field>
            <field name="view_id" ref="view_onpoint_logger_map"/>
            <field name="act_window_id" ref="act_onpoint_logger"/>
        </record>


    </data>
</odoo>