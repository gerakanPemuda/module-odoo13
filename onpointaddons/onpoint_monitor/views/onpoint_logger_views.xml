                                                                    <td>Hi Hi</td>
                                                                    <td>
                                                                        <field name="hi_hi_threshold" nolabel="1"/>
                                                                    </td>
                                                                </tr>
                                                                <tr class="o_data_row">
                                                                    <td style="text-align:center">
                                                                        <field name="hi_enabled" nolabel="1"/>
                                                                    </td>
                                                                    <td>Hi</td>
                                                                    <td>
                                                                        <field name="hi_threshold" nolabel="1"/>
                                                                    </td>
                                                                </tr>
                                                                <tr class="o_data_row">
                                                                    <td style="text-align:center">
                                                                        <field name="lo_enabled" nolabel="1"/>
                                                                    </td>
                                                                    <td>Lo</td>
                                                                    <td>
                                                                        <field name="lo_threshold" nolabel="1"/>
                                                                    </td>
                                                                </tr>
                                                                <tr class="o_data_row">
                                                                    <td style="text-align:center">
                                                                        <field name="lo_lo_enabled" nolabel="1"/>
                                                                    </td>
                                                                    <td>Lo Lo</td>
                                                                    <td>
                                                                        <field name="lo_lo_threshold" nolabel="1"/>
                                                                    </td>
                                                                </tr>
                                                                <tr class="o_data_row">
                                                                    <td style="text-align:center">
                                                                        <field name="underrange_enabled" nolabel="1"/>
                                                                    </td>
                                                                    <td>Underrange</td>
                                                                    <td>
                                                                        <field name="underrange_threshold" nolabel="1"/>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </group>
                                                </page>
                                                <page string="Field Verification"
                                                      attrs="{'invisible': [('value_type_name', 'not in', ('Flow'))]}">
                                                    <group>
                                                        <field name="initial_ids" nolabel="1" colspan="4"
                                                               widget="one2many_list">
                                                            <tree editable="bottom">
                                                                <field name="dates"/>
                                                                <field name="initial" string="Meter Index"/>
                                                            </tree>
                                                        </field>
                                                    </group>
                                                </page>
                                            </notebook>

                                        </form>
                                    </field>
                                </group>

                            </page>
                            <page name="profile" string="Profile">
                                <group>
                                    <group>
                                        <field name="meter_type_id"/>
                                        <field name="meter_brand_id"/>
                                        <field name="meter_size_id"/>
                                    </group>
                                    <group>
                                        <field name="pipe_material_id"/>
                                        <field name="pipe_size_id"/>
                                        <field name="valve_control_id"/>
                                    </group>
                                </group>

                            </page>
                            <page name="other" string="Other">

                            </page>

                            <!-- <page string="Threshold">
                                <group>

                                    <group>
                                        <div class="col-md-11">
                                            <field name="threshold_ids" nolabel="1" colspan="4" widget="one2many_list" >
                                                <tree string="Logger" editable="bottom">
                                                    <field name="value_type_id"/>
                                                    <field name="min_value" />
                                                    <field name="max_value" />
                                                </tree>
                                            </field>
                                        </div>
                                    </group>

                                    <group string="Leakage">
                                        <field name="leakage" />
                                        <field name="leakage_interval_minutes" />
                                        <field name="leakage_interval_days" />
                                    </group>
                                </group>
                            </page> -->

                            <page string="Threshold Hourly"
                                  attrs="{'invisible': [('is_threshold_hourly', '=', False)]}">
                                <group>
                                    <field name="threshold_hourly_ids" nolabel="1" colspan="4" widget="one2many_list">
                                        <tree string="Logger" editable="bottom" create="false" delete="false">
                                            <field name="hours"/>
                                            <field name="min_value"/>
                                            <field name="max_value"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <!-- <div class="col-sm-3">
                                <separator string="Channel 1"/>
                                <group>
                                    <field name="ch1_active" string="Active"/>

                                    <div class="oe_title" colspan="4" attrs="{'invisible': [('ch1_active', '=', False)]}">
                                        <label for="ch1_value_type_id" style="font-weight:bold !important"/>
