<odoo>

    <data>

        <record id="view_onpoint_logger_tree" model="ir.ui.view">
            <field name="name">onpoint.logger.tree</field>
            <field name="model">onpoint.logger</field>
            <field name="arch" type="xml">
                <tree string="Logger">
                    <field name="name"/>
                    <field name="identifier"/>
                    <field name="logger_type_id"/>
                    <field name="dma_id"/>
                    <field name="brand_id"/>
                    <field name="last_data_date"/>
                    <field name="is_still_active"/>
                </tree>
            </field>
        </record>

        <record id="view_onpoint_logger_form" model="ir.ui.view">
            <field name="name">onpoint.logger.form</field>
            <field name="model">onpoint.logger</field>
            <field name="arch" type="xml">
                <form string="Logger">
                    <header style="padding:5px">
                        <button
                                class="oe_highlight"
                                string="Add to Highlights"
                                type="object"
                                name="act_add_to_dashboard"
                                style="margin-left:20px"
                                attrs="{'invisible': [('is_on_dashboard', '=', True)]}"
                        />

                        <button
                                class="oe_highlight"
                                string="Remove from Highlights"
                                type="object"
                                name="act_remove_from_dashboard"
                                style="margin-left:20px"
                                attrs="{'invisible': [('is_on_dashboard', '=', False)]}"
                        />

                        <button
                                class="btn btn-success"
                                string="Enable Logger"
                                type="object"
                                name="act_enable_logger"
                                style="margin-left:20px"
                                attrs="{'invisible': [('state', '=', 'enabled')]}"
                        />

                        <button
                                class="btn btn-danger"
                                string="Disable Logger"
                                type="object"
                                name="act_disable_logger"
                                style="margin-left:20px"
                                attrs="{'invisible': [('state', '=', 'disabled')]}"
                        />

                        <field name="state" widget="statusbar"/>

                    </header>

                    <div style="padding-left:10px; padding-right:10px">

                        <div class="oe_button_box" name="button_box">
                            <button class="oe_stat_button" name="action_to_logger_value" type="object" icon="fa-pencil">
                                <field name="count_logger_value" string="Logger Value" widget="statinfo"/>
                            </button>

                            <button
                                    class="oe_stat_button o_res_partner_tip_opp"
                                    name="%(act_view_logger_chart)d"
                                    context="{'default_id': id}"
                                    type="action"
                                    icon="fa-chart-line">
                                Chart
                            </button>

                        </div>

                        <field name="is_on_dashboard" invisible="1"/>
                        <field name="image_1920" widget='image' class="oe_avatar"
                               options='{"preview_image": "image_128"}'/>
                        <div class="oe_title">
                            <label for="name"/>
                            <h1>
                                <field name="name" class="oe_inline"/>
                            </h1>
                        </div>

                        <group>
                            <group>
                                <separator string="Logger Information" colspan="2" style="margin-bottom:10px"/>
                                <field name="identifier"/>
                                <field name="brand_id" options="{'no_create': True, 'no_create_edit': True}"
                                       widget="selection"/>
                                <field name="logger_type_id"/>
                                <field name="department_id"/>
                                <field name="wtp_id"/>
                                <field name="zone_id"/>
                                <field name="dma_id"/>
                            </group>
                            <group>
                                <separator string="Location and Communication" colspan="2" style="margin-bottom:10px"/>
                                <field name="brand_owner" invisible="1"/>
                                <field name="simcard"/>
                                <field name="address" colspan="4"/>
                                <field name="latitude"/>
                                <field name="longitude"/>
                                <field name="elevation"/>
                                <field name="is_threshold_hourly" invisible="True"/>
                                <field name="show_all_channels" groups="onpoint_monitor.group_onpoint_monitor_superadmin"/>
                            </group>

                        </group>

                        <group col="2" colspan="4">
                            <field name="remarks" colspan="4"/>
                        </group>

                        <notebook>
                            <page name="channel" string="Channel">
                                <button
                                        class="oe_highlight oe_read_only"
                                        string="Toggle Channels"
                                        type="object"
                                        name="act_toggle_channels"
                                        style="margin-top:10px; float: right"
                                        groups="onpoint_monitor.group_onpoint_monitor_superadmin"
                                />

                                <group>
                                    <field name="channel_ids"
                                           nolabel="1"
                                           colspan="4"
                                           widget="one2many_list"
                                           mode="kanban"
                                           context="{'default_brand_owner': brand_owner}">

                                        <kanban>
                                            <field name="id"/>
                                            <field name="color"/>
                                            <field name="name"/>
                                            <field name="value_type_id"/>
                                            <field name="value_unit_id"/>
                                            <field name="value_unit_name"/>
                                            <field name="point_id"/>
                                            <field name="point_is_sensor"/>
                                            <field name="last_date"/>
                                            <field name="last_value"/>
                                            <field name="last_value_type"/>
                                            <field name="show_channel"/>
                                            <templates>
                                                <t t-if="record.show_channel.raw_value" t-name="kanban-box">
                                                    <t t-set="color"
                                                       t-value="kanban_color(record.color.raw_value)"/>
<!--                                                    <t t-if="record.value_unit_id.raw_value">-->
                                                        <div t-att-class="'oe_kanban_global_click'">
                                                            <div class="oe_kanban_details">
                                                                <div class="clearfix">
                                                                    <strong class="float-left"
                                                                            t-attf-style="color: {{ record.color.raw_value }}">
                                                                        <field name="name"/>
                                                                    </strong>
                                                                    <strong class="float-right">
                                                                        <field name="value_type_id"/>
                                                                        <!--                                                                    <field name="point_id"/>-->
                                                                    </strong>
                                                                </div>
                                                                <div class="d-flex justify-content-center"
                                                                     t-if="record.last_value.raw_value">
                                                                    <span class="display-4">
                                                                        <field name="last_value"/>
                                                                        <field name="value_unit_id"/>
                                                                    </span>
                                                                </div>
                                                                <div class="d-flex justify-content-center"
                                                                     t-if="record.last_value.raw_value">
                                                                    <h2>
                                                                        <small>
                                                                            <field name="last_date"/>
                                                                        </small>
                                                                    </h2>
                                                                </div>


                                                            </div>
                                                        </div>
<!--                                                    </t>-->
                                                </t>
                                            </templates>
                                        </kanban>

                                        <form>
                                            <header>

                                            </header>
                                            <group>
                                                <group name="bio">
                                                    <field name="value_type_name" invisible="1"/>
                                                    <field name="brand_owner" invisible="1"/>
                                                    <field name="name"/>
                                                    <field name="point_id"
                                                           options="{'no_create': True, 'no_create_edit': True}"
                                                           widget="selection"/>
                                                    <field name="value_type_id"/>
                                                    <field name="value_unit_id"
                                                           context="{'form_view_ref': 'onpoint_monitor.view_onpoint_value_unit_popup_form', 'default_value_type_id': value_type_id}"/>
                                                    <field name="color" widget="color"/>
                                                    <field name="display_on_chart"/>
                                                    <field name="show_channel" groups="onpoint_monitor.group_onpoint_monitor_superadmin"/>
                                                    <field name="show_consumption"/>
                                                </group>

                                                <group name="bio2">
                                                    <label for="interval_minutes"/>
                                                    <div>
                                                        <field name="interval_minutes" nolabel="1" widget="integer"
                                                               class="oe_inline" style="width:50px !important"/>
                                                        minutes
                                                    </div>
                                                    <field name="interval" invisible="1"/>
                                                </group>
                                            </group>

                                            <notebook>
                                                <page string="Threshold">
                                                    <group name="threshold" colspan="4">
                                                        <table class="o_list_view table table-condensed table-striped o_list_view_ungrouped"
                                                               width="80%">
                                                            <thead>
                                                                <tr>
                                                                    <th class="o_column_sortable" width="15%">Enabled
                                                                    </th>
                                                                    <th class="o_column_sortable" width="50%">Level</th>
                                                                    <th class="o_column_sortable" width="35%">Value</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr class="o_data_row">
                                                                    <td style="text-align:center">
                                                                        <field name="overrange_enabled" nolabel="1"/>
                                                                    </td>
                                                                    <td>Overrange</td>
                                                                    <td>
                                                                        <field name="overrange_threshold" nolabel="1"/>
                                                                    </td>
                                                                </tr>
                                                                <tr class="o_data_row">
                                                                    <td style="text-align:center">
                                                                        <field name="hi_hi_enabled" nolabel="1"/>
